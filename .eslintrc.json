{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"prettier",
		"plugin:@typescript-eslint/recommended",
		"standard"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
		"no-template-curly-in-string": "off"
	}
}